<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.escritorio.merkapp.FragmentInicio">

    <!-- TODO: Update blank fragment layout -->

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/btnflo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:visibility="gone"
        app:fabSize="normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar2"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/vadd"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="8dp" />

    <android.support.v4.widget.ContentLoadingProgressBar
        android:id="@+id/progress"
        style="@style/Base.Widget.AppCompat.ProgressBar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_gravity="center"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="@+id/lista"
        app:layout_constraintLeft_toLeftOf="@+id/lista"
        app:layout_constraintRight_toRightOf="@+id/lista"
        app:layout_constraintTop_toTopOf="@+id/lista" />

    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:background="?attr/colorPrimary"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:title="Menu"
        app:titleTextColor="@android:color/white" />

    <ImageView
        android:id="@+id/imgpasta"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:background="@drawable/borden"
        android:padding="5dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="@+id/imgcarne"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.67"
        app:layout_constraintLeft_toRightOf="@+id/imgchi"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imgcarne"
        app:layout_constraintTop_toTopOf="@+id/imgcarne"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/imgpollo"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginBottom="4dp"
        android:background="@drawable/borden"
        android:padding="5dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="@+id/imgcarne"
        app:layout_constraintEnd_toStartOf="@+id/imgcarne"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/imgchi"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imgCarn"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:id="@+id/imgcarne"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="36dp"
        android:layout_marginRight="8dp"
        android:background="@drawable/borden"
        android:padding="5dp"
        android:scaleType="fitXY"
        app:layout_constraintHorizontal_bias="0.526"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar2" />

    <ImageView
        android:id="@+id/imgmaris"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="156dp"
        android:layout_marginRight="8dp"
        android:background="@drawable/borden"
        android:padding="5dp"
        android:scaleType="fitXY"
        app:layout_constraintHorizontal_bias="0.526"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar2" />

    <ImageView
        android:id="@+id/imglic"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="288dp"
        android:layout_marginRight="8dp"
        android:background="@drawable/borden"
        android:padding="5dp"
        android:scaleType="fitXY"
        app:layout_constraintHorizontal_bias="0.526"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar2" />

    <ImageView
        android:id="@+id/imgpica"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:background="@drawable/borden"
        android:padding="5dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="@+id/imglic"
        app:layout_constraintEnd_toStartOf="@+id/imglic"
        app:layout_constraintHorizontal_bias="0.51"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imglic"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/imglasa"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:background="@drawable/borden"
        android:padding="5dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="@+id/imgmaris"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.623"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imgmaris"
        app:layout_constraintTop_toTopOf="@+id/imgmaris"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/imgseco"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:background="@drawable/borden"
        android:padding="5dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="@+id/imgmaris"
        app:layout_constraintEnd_toStartOf="@+id/imgmaris"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imgmaris"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:text="CARNE"
        app:layout_constraintEnd_toEndOf="@+id/imgcarne"
        app:layout_constraintHorizontal_bias="0.485"
        app:layout_constraintLeft_toLeftOf="@+id/imgchi"
        app:layout_constraintRight_toRightOf="@+id/imgchi"
        app:layout_constraintStart_toStartOf="@+id/imgCarn"
        app:layout_constraintTop_toBottomOf="@+id/imgcarne" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="12dp"
        android:text="MARISCOS"
        app:layout_constraintEnd_toEndOf="@+id/imgmaris"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="@+id/imgchi"
        app:layout_constraintRight_toRightOf="@+id/imgchi"
        app:layout_constraintStart_toStartOf="@+id/imgmaris"
        app:layout_constraintTop_toBottomOf="@+id/imgmaris" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="20dp"
        android:text="LICORES"
        app:layout_constraintEnd_toEndOf="@+id/imglic"
        app:layout_constraintHorizontal_bias="0.04"
        app:layout_constraintLeft_toLeftOf="@+id/imgchi"
        app:layout_constraintRight_toRightOf="@+id/imgchi"
        app:layout_constraintStart_toStartOf="@+id/imglic"
        app:layout_constraintTop_toBottomOf="@+id/imglic" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="POLLO"
        app:layout_constraintEnd_toEndOf="@+id/imgpollo"
        app:layout_constraintHorizontal_bias="0.432"
        app:layout_constraintLeft_toLeftOf="@+id/imghami"
        app:layout_constraintRight_toRightOf="@+id/imghami"
        app:layout_constraintStart_toStartOf="@+id/imgpollo"
        app:layout_constraintTop_toBottomOf="@+id/imgpollo" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="PICAD. Y ENSALADA"
        app:layout_constraintEnd_toEndOf="@+id/imgpica"
        app:layout_constraintHorizontal_bias="0.551"
        app:layout_constraintLeft_toLeftOf="@+id/imghami"
        app:layout_constraintRight_toRightOf="@+id/imghami"
        app:layout_constraintStart_toStartOf="@+id/imgpica"
        app:layout_constraintTop_toBottomOf="@+id/imgpica" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="SECOS"
        app:layout_constraintEnd_toEndOf="@+id/imgseco"
        app:layout_constraintHorizontal_bias="0.432"
        app:layout_constraintLeft_toLeftOf="@+id/imghami"
        app:layout_constraintRight_toRightOf="@+id/imghami"
        app:layout_constraintStart_toStartOf="@+id/imgseco"
        app:layout_constraintTop_toBottomOf="@+id/imgseco" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="LAZAÑAS"
        app:layout_constraintEnd_toEndOf="@+id/imglasa"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintLeft_toLeftOf="@+id/imgpizzai"
        app:layout_constraintRight_toRightOf="@+id/imgpizzai"
        app:layout_constraintStart_toStartOf="@+id/imglasa"
        app:layout_constraintTop_toBottomOf="@+id/imglasa" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="12dp"
        android:text="PASTA"
        app:layout_constraintEnd_toEndOf="@+id/imgpasta"
        app:layout_constraintHorizontal_bias="0.486"
        app:layout_constraintLeft_toLeftOf="@+id/imgpizzai"
        app:layout_constraintRight_toRightOf="@+id/imgpizzai"
        app:layout_constraintStart_toStartOf="@+id/imgpasta"
        app:layout_constraintTop_toBottomOf="@+id/imgpasta" />

    <android.support.v7.widget.RecyclerView
        android:id="@+id/lista"
        android:layout_width="382dp"
        android:layout_height="222dp"


        android:layout_marginLeft="8dp"
        android:layout_marginTop="190dp"
        android:layout_marginRight="8dp"
        android:drawingCacheQuality="auto"
        android:transitionGroup="false"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/toolbar2"
        app:layout_constraintVertical_bias="0.93" >

    </android.support.v7.widget.RecyclerView>

    <ImageView
        android:id="@+id/btnsalir"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/toolbar2"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintLeft_toLeftOf="@+id/toolbar2"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/vexit"

        />

    <ImageView
        android:id="@+id/btnubi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="8dp"
        android:src="@android:drawable/ic_dialog_map"
        app:layout_constraintBottom_toBottomOf="@+id/toolbar2"
        app:layout_constraintHorizontal_bias="0.881"
        app:layout_constraintLeft_toLeftOf="@+id/toolbar2"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.166" />

</android.support.constraint.ConstraintLayout>
